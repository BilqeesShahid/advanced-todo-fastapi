{
  "version": 2,
  "builds": [
    {
      "src": "app/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.10"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/wsgi.py"
    }
  ],
  "regions": ["iad1"],
  "github": {
    "enabled": true
  }
}
